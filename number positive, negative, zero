#include <stdio.h>
void main()
{
    int x;
 
    printf("Input a number :");
    scanf("%d", &x);
    if(x>0)
    {
        printf("Number is POSITIVE");
    }
   else
    if(x<0)
    {
        printf("Number is NEGATIVE");
    }
   else
   if(x==0)
    {
        printf("Number is ZERO");
    }
   }
